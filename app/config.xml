<?xml version='1.0' encoding='utf-8'?>
<widget id="co.blucocoondigital.healthapp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Ingredion Care First</name>
    <description>Health App for Ingredion</description>
    <author email="ideas@neutrinos.co" href="http://neutrinos.co">
        Neutrinos Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="OverrideUserAgent" value="Mozilla/5.0 Google" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="src/assets/Android/res/drawable/icon3.png" />
        <icon density="ldpi" src="src/assets/Android/res/drawable-ldpi/icon3.png" />
        <icon density="mdpi" src="src/assets/Android/res/drawable-mdpi/icon3.png" />
        <icon density="hdpi" src="src/assets/Android/res/drawable-hdpi/icon3.png" />
        <icon density="xhdpi" src="src/assets/Android/res/drawable-xhdpi/icon3.png" />
        <icon density="xxhdpi" src="src/assets/Android/res/drawable-xxhdpi/icon3.png" />
        <icon density="xxxhdpi" src="src/assets/Android/res/drawable-xxxhdpi/icon3.png" />
        <splash src="src/assets/Android/res/drawable/screen2.png" />
        <splash src="src/assets/Android/res/drawable-land/screen2.png" />
        <splash density="ldpi" src="src/assets/Android/res/drawable-ldpi/screen2.png" />
        <splash density="land-ldpi" src="src/assets/Android/res/drawable-land-ldpi/screen2.png" />
        <splash density="mdpi" src="src/assets/Android/res/drawable-mdpi/screen2.png" />
        <splash density="land-mdpi" src="src/assets/Android/res/drawable-land-mdpi/screen2.png" />
        <splash density="hdpi" src="src/assets/Android/res/drawable-hdpi/screen2.png" />
        <splash density="land-hdpi" src="src/assets/Android/res/drawable-land-hdpi/screen2.png" />
        <splash density="xhdpi" src="src/assets/Android/res/drawable-xhdpi/screen2.png" />
        <splash density="land-xhdpi" src="src/assets/Android/res/drawable-land-xhdpi/screen2.png" />
        <splash density="xxhdpi" src="src/assets/Android/res/drawable-xxhdpi/screen2.png" />
        <splash density="land-xxhdpi" src="src/assets/Android/res/drawable-land-xxhdpi/screen2.png" />
        <splash density="xxxhdpi" src="src/assets/Android/res/drawable-xxxhdpi/screen2.png" />
        <splash density="land-xxxhdpi" src="src/assets/Android/res/drawable-land-xxxhdpi/screen2.png" />
        <resource-file src="google-services.json" target="app/google-services.json" />
        <hook src="scripts/beforeBuild.js" type="before_build" />
    </platform>
    <preference name="SplashScreen" value="screen" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="orientation" value="portrait" />
    <feature name="SplashScreen">
        <param name="ios-package" value="CDVSplashScreen" />
        <param name="android-package" value="org.apache.cordova.SplashScreen" />
    </feature>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="webviewbounce" value="false" />
        <preference name="UIWebViewBounce" value="false" />
        <preference name="DisallowOverscroll" value="true" />
        <preference name="FadeSplashScreen" value="false" />
        <preference name="AutoHideSplashScreen" value="false" />
        <resource-file src="GoogleService-Info.plist" />
        <splash height="1136" src="src/assets/iOS/Resources/splash/Default-568h@2x~iphone.png" width="640" />
        <splash height="1334" src="src/assets/iOS/Resources/splash/Default-667h.png" width="750" />
        <splash height="2208" src="src/assets/iOS/Resources/splash/Default-736h.png" width="1242" />
        <splash height="1242" src="src/assets/iOS/Resources/splash/Default-Landscape-736h.png" width="2208" />
        <splash height="1536" src="src/assets/iOS/Resources/splash/Default-Landscape@2x~ipad.png" width="2048" />
        <splash height="2048" src="src/assets/iOS/Resources/splash/Default-Landscape@~ipadpro.png" width="2732" />
        <splash height="768" src="src/assets/iOS/Resources/splash/Default-Landscape~ipad.png" width="1024" />
        <splash height="2048" src="src/assets/iOS/Resources/splash/Default-Portrait@2x~ipad.png" width="1536" />
        <splash height="2732" src="src/assets/iOS/Resources/splash/Default-Portrait@~ipadpro.png" width="2048" />
        <splash height="1024" src="src/assets/iOS/Resources/splash/Default-Portrait~ipad.png" width="768" />
        <splash height="960" src="src/assets/iOS/Resources/splash/Default@2x~iphone.png" width="640" />
        <splash height="480" src="src/assets/iOS/Resources/splash/Default~iphone.png" width="320" />
        <splash height="2732" src="src/assets/iOS/Resources/splash/Default@2x~universal~anyany.png" width="2732" />
    
        <icon height="180" src="src/assets/iOS/Resources/icons/icon-60@3x.png" width="180" />
        <icon height="60" src="src/assets/iOS/Resources/icons/icon-60.png" width="60" />
        <icon height="120" src="src/assets/iOS/Resources/icons/icon-60@2x.png" width="120" />
        <icon height="76" src="src/assets/iOS/Resources/icons/icon-76.png" width="76" />
        <icon height="152" src="src/assets/iOS/Resources/icons/icon-76@2x.png" width="152" />
        <icon height="40" src="src/assets/iOS/Resources/icons/icon-40.png" width="40" />
        <icon height="80" src="src/assets/iOS/Resources/icons/icon-40@2x.png" width="80" />
        <icon height="57" src="src/assets/iOS/Resources/icons/icon.png" width="57" />
        <icon height="114" src="src/assets/iOS/Resources/icons/icon@2x.png" width="114" />
        <icon height="72" src="src/assets/iOS/Resources/icons/icon-72.png" width="72" />
        <icon height="144" src="src/assets/iOS/Resources/icons/icon-72@2x.png" width="144" />
        <icon height="29" src="src/assets/iOS/Resources/icons/icon-small.png" width="29" />
        <icon height="58" src="src/assets/iOS/Resources/icons/icon-small@2x.png" width="58" />
        <icon height="50" src="src/assets/iOS/Resources/icons/icon-50.png" width="50" />
        <icon height="100" src="src/assets/iOS/Resources/icons/icon-50@2x.png" width="100" />
        <config-file parent="UIFileSharingEnabled" target="*-Info.plist">
            <true />
        </config-file>
    </platform>
    <plugin name="cordova-plugin-device" spec="2.0.2" />
    <plugin name="cordova-plugin-inappbrowser" spec="3.1.0" />
    <plugin name="cordova-plugin-splashscreen" spec="5.0.2" />
    <plugin name="cordova-plugin-whitelist" spec="1.3.3" />
    <plugin name="cordova-plugin-file" spec="6.0.1" />
    <config-file parent="NSBluetoothAlwaysUsageDescription" platform="ios" target="*-Info.plist">
        <string>This app requires access to function properly</string>
    </config-file>
    <plugin name="cordova-plugin-uniquedeviceid" spec="1.3.2" />
    <plugin name="com.cordova.plugins.cookiemaster" spec="1.0.1" />
    <engine name="android" spec="7.1.4" />
    <engine name="ios" spec="5.0.1" />
</widget>
